{
  "expo": {
    "name": "GameDex",
    "slug": "GameDex",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/logo/gamedex-icon-transparent.png",
    "splash": {
      "image": "./assets/logo/gamedex-transparent.png",
      "resizeMode": "cover",
      "backgroundColor": "#252525",
      "autoHide": false
    },
    "scheme": "GameDex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/gamedex-transparent.png",
        "backgroundColor": "#252525"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/gamedex-transparent.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#252525"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}